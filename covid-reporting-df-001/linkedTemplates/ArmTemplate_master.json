{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-df-001"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"ls_blob_covidreporting_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://strgaccount013.blob.core.windows.net"
		},
		"ls_datalake_covidreporting_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://coviddatalake019.dfs.core.windows.net"
		},
		"ls_http_git_covidReports_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"trg_create_source_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4ff3d32b-bb1d-42aa-8a7f-9078cde6afac/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/strgaccount013"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-df-001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"ls_blob_covidreporting_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_blob_covidreporting_properties_typeProperties_serviceEndpoint')]"
					},
					"ls_datalake_covidreporting_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_covidreporting_properties_typeProperties_url')]"
					},
					"ls_http_git_covidReports_properties_typeProperties_url": {
						"value": "[parameters('ls_http_git_covidReports_properties_typeProperties_url')]"
					},
					"trg_create_source_properties_typeProperties_scope": {
						"value": "[parameters('trg_create_source_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-reporting-df-001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-reporting-df-001_ArmTemplate_0')]"
			]
		}
	]
}